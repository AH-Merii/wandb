id: 0.monai.classification
plugin:
    - wandb
tag:
  platforms:
    - linux
command:
    program: test_monai_classification.py
depend:
    requirements:
        - "monai['all']>=1.3.0"
assert:
    - :wandb:runs_len: 1
    - :wandb:runs[0][config][batch_size]: 2
    - :wandb:runs[0][config][lr]: 0.00001
    - :wandb:runs[0][config][num_workers]: 4
    - :wandb:runs[0][config][train_epochs]: 2
    - :wandb:runs[0][config][validation_every_n_epochs]: 1
    - :op:>=:
        - :wandb:runs[0][summary][epoch/AUC]
        - 0
    - :op:==:
        - :wandb:runs[0][summary][epoch/epoch]
        - 2
    - :op:>=:
        - :wandb:runs[0][summary][iteration/Loss]
        - 0
    - :op:==:
        - :wandb:runs[0][summary][iteration/iteration]
        - 10
    - :wandb:runs[0][exitcode]: 0
